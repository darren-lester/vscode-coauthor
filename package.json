{
    "name": "git-coauthors",
    "displayName": "git-coauthors",
    "description": "Easily add/remove coauthors from a developing session",
    "version": "0.0.1",
    "publisher": "nones",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.selectRepo",
                "title": "Change repo for pairing candidates"
            },
            {
                "command": "extension.addBuddy",
                "title": "Change pairing buddies"
            },
            {
                "command": "extension.stopPairing",
                "title": "Stop pairing"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
